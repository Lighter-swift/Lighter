{
  "__doc__": "Configuration used for the manual, builtin codegen.",
  
  "databaseExtensions" : [ "sqlite3", "db", "sqlite" ],
  "sqlExtensions"      : [ "sql" ],
  
  "CodeStyle": {
    "functionCommentStyle" : "**",
    "indent"               : "  ",
    "lineLength"           : 80
  },
  
  "EmbeddedLighter": {
    "selects": {
      "syncYield"  : "none",
      "syncArray"  : { "columns": 6, "sorts": 2 },
      "asyncArray" : { "columns": 6, "sorts": 2 }
    },
    "updates": {
      "keyBased"       : 6,
      "predicateBased" : 6
    },
    "inserts": 6
  }
}
